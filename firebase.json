{
  "functions": {
    "source": "functions",
    "ignore": [
      "**/node_modules/**",
      "**/serviceAccountKey*",
      "**/src/**",
      "*.log",
      "**/.gitignore",
      "**/.template.env",
      "**/.tsconfig.json",
      "**/utils/encode-key.js"
    ]
  },
  "hosting": {
    "public": "public",
    "rewrites": [{ "source": "**", "function": "api" }]
  }
}
